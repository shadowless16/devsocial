    "build:stats": "cross-env ANALYZE=true next build",
    "build:fast": "cross-env NODE_OPTIONS=--max-old-space-size=8192 next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "check": "pnpm run lint && pnpm run build",
    "check-routes": "node scripts/find-route-errors.js",
    "test": "jest",
    "test:fast": "jest __tests__/referrals/referral-system.test.ts __tests__/referrals/referral-api-fixed.test.ts __tests__/analytics/growth-simple.test.ts --runInBand --forceExit",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=\"__tests__/(utils|components|auth|analytics)\"",
    "test:integration": "jest --testPathPattern=\"__tests__/integration\"",
    "test:analytics": "jest __tests__/analytics",
    "test:auth": "jest __tests__/auth",
    "test:referrals": "jest __tests__/referrals",
    "test:ci": "jest --ci --coverage --watchAll=false --passWithNoTests",
    "test:runner": "node test-runner.js",
    "test:clean": "rimraf coverage jest-cache && jest --clearCache",
    "test:imprint": "jest __tests__/canonicalizer.test.ts __tests__/imprint-worker.test.ts",
    "test:verify": "jest __tests__/verify-endpoint.test.ts",
