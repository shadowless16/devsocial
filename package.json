  "name": "devsocial",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--max-old-space-size=8192 next dev --turbo",
    "dev:fast": "cross-env NODE_OPTIONS=--max-old-space-size=8192 NEXT_CONFIG_FILE=next.config.fast.mjs next dev --turbo",
    "dev:turbo": "cross-env NODE_OPTIONS=--max-old-space-size=8192 NEXT_CONFIG_FILE=next.config.turbo.mjs next dev --turbo",
