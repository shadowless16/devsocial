    "test:growth": "jest __tests__/analytics/growth-simple.test.ts",
    "test:mcp": "jest __tests__/analytics/mcp-growth.test.ts",
    "generate:test-data": "node scripts/generate-test-growth-data.js",
    "dev:with-mcp": "concurrently \"npm run mcp:dev\" \"npm run dev\"",
    "ts-node:run": "ts-node -r tsconfig-paths/register --files --transpile-only",
    "debug:referrals": "node scripts/debug-referral-node.js",
    "test:referral-signup": "node scripts/test-referral-signup-flow.js",
    "mcp:referral-debug": "node mcp-server/referral-debug.js",
